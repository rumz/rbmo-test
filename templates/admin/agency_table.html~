<table class="table table-condensed">
  <thead>
    <th>Agency/Office</th>
    <th>Email</th>
    <th>Sector</th>
  </thead>
  <tbody>
    {%for agency in agencies%}
    <tr onmouseover="$(this).addClass('warning')" onmouseout="$(this).removeClass()" style="cursor: pointer">
      <td>{{agency.name}}</td>
      <td>{{agency.email}}</td>
      <td>{{agency.sector.name}}</td>
    </tr>
    {%endfor%}
  </tbody>
</table>
